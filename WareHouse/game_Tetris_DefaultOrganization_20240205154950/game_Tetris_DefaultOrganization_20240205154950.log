class Piece:
    def __init__(self, shape, x, y):
        self.shape = shape
        self.x = x
        self.y = y
    def move_left(self):
        return Piece(self.shape, self.x - 1, self.y)
    def move_right(self):
        return Piece(self.shape, self.x + 1, self.y)
    def move_down(self):
        return Piece(self.shape, self.x, self.y + 1)

        self.shape = shape
        self.x = x
        self.y = y

    def move_left(self):
        return Piece(self.shape, self.x - 1, self.y)

    def move_right(self):
        return Piece(self.shape, self.x + 1, self.y)

    def move_down(self):
        return Piece(self.shape, self.x, self.y + 1)

    def rotate(self):
        rotated_shape = [[0] * 4 for _ in range(4)]
        for row in range(4):
            for col in range(4):
                rotated_shape[col][3 - row] = self.shape[row][col]
        return Piece(rotated_shape, self.x, self.y)
